{
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "type": "constrained",
            "contentSize": "800px",
            "wideSize": "1408px"
        },
        "color": {
            "palette": [
                {
                    "name": "Dark",
                    "slug": "dark",
                    "color": "#212529"
                },
                {
                    "name": "Gray",
                    "slug": "gray",
                    "color": "#676767"
                },
                {
                    "name": "Green",
                    "slug": "green",
                    "color": "#00730C"
                },
                {
                    "name": "Dark Green",
                    "slug": "dark-green",
                    "color": "#0C4B35"
                },
                {
                    "name": "Light Green",
                    "slug": "light-green",
                    "color": "#E6F6EA"
                }
            ],
            "gradients": [
                {
                    "slug": "plants-gradient-1",
                    "gradient": "linear-gradient(6.68deg, #00730C 4.95%, rgba(251, 246, 238, 0) 264.79%)",
                    "name": "Plants gradient 1"
                }
            ]
        },
        "custom": {
            "font-size-h1": "clamp(34.54px, 2.158rem + ((1vw - 3.9px) * 2.417), 58px)",
            "font-size-h2": "clamp(26.8px, 1.674rem + ((1vw - 3.9px) * 1.874), 45px)",
            "font-size-h3": "clamp(18px, 1.1rem + ((1vw - 3.9px) * 0.7), 24px)",
            "font-size-h4": "clamp(16px, 1rem + ((1vw - 3.9px) * 0.2), 18px)",
            "font-size-body": "clamp(16px, 1rem + ((1vw - 3.9px) * 0.2), 18px)",
            "spacing-15": "15px"
        },
        "spacing": {
            "blockGap": true,
            "padding": true,
            "margin": true,
            "units": [
                "px",
                "em",
                "rem",
                "%"
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "name": "Playfair Display",
                    "slug": "playfair-display",
                    "fontFamily": "\"Playfair Display\", serif",
                    "fontFace": [
                        {
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/src/fonts/PlayfairDisplay/PlayfairDisplay-Regular.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": [
                                "file:./assets/src/fonts/PlayfairDisplay/PlayfairDisplay-Medium.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "600",
                            "src": [
                                "file:./assets/src/fonts/PlayfairDisplay/PlayfairDisplay-SemiBold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/src/fonts/PlayfairDisplay/PlayfairDisplay-Bold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Playfair Display",
                            "fontStyle": "normal",
                            "fontWeight": "800",
                            "src": [
                                "file:./assets/src/fonts/PlayfairDisplay/PlayfairDisplay-ExtraBold.ttf"
                            ]
                        }
                    ]
                },
                {
                    "name": "Roboto",
                    "slug": "roboto",
                    "fontFamily": "\"Roboto\", sans-serif",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/src/fonts/Roboto/Roboto-Regular.ttf"
                            ],
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontFamily": "Roboto"
                        },
                        {
                            "src": [
                                "file:./assets/src/fonts/Roboto/Roboto-Bold.ttf"
                            ],
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontFamily": "Roboto"
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "clamp(16px, 1rem + ((1vw - 3.9px) * 0.2), 18px)",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "clamp(18px, 1.1rem + ((1vw - 3.9px) * 0.7), 24px)",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "clamp(26.8px, 1.674rem + ((1vw - 3.9px) * 1.874), 45px)",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "clamp(34.54px, 2.158rem + ((1vw - 3.9px) * 2.417), 58px)",
                    "name": "Extra Large"
                }
            ],
            "defaultFontSizes": false,
            "fluid": false,
            "lineHeight": true
        },
        "shadow": {
            "presets": [
                {
                    "name": "Shadow 1",
                    "slug": "shadow-1",
                    "shadow": "2px 11px 30px rgba(51, 51, 51, 0.08)"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "core/button": {
                "border": {
                    "radius": "5px",
                    "width": "1px",
                    "style": "solid",
                    "color": "var(--wp--preset--color--green)"
                },
                "color": {
                    "background": "var(--wp--preset--color--green)",
                    "text": "var(--wp--preset--color--white)"
                },
                "spacing": {
                    "padding": {
                        "top": "10px",
                        "right": "35px",
                        "bottom": "10px",
                        "left": "35px"
                    }
                }
            },
            "core/navigation": {
                "elements": {
                    "link": {
                        "typography": {
                            "fontSize": "16px",
                            "fontWeight": "700",
                            "lineHeight": "20px"
                        }
                    }
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto)",
            "fontSize": "var(--wp--custom--font-size-body)",
            "lineHeight": "1.4"
        },
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--playfair-display)",
                    "fontWeight": "700"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size-h-1)",
                    "fontWeight": "800"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size-h-2)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size-h-3)",
                    "fontWeight": "500"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size-h-4)"
                }
            }
        }
    },
    "customTemplates": [
        {
            "name": "page-no-title",
            "title": "The template without title",
            "postTypes": [
                "page",
                "post"
            ]
        }
    ],
    "patterns": [
        "about",
        "logos",
        "staff",
        "we-have-solutions",
        "working-process",
        "why-choose-us"
    ]
}